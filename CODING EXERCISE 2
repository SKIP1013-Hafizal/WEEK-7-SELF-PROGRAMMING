package exercise;

public class exercise2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int k=20;
		int m=20;
		int n=20;
		while (k > 10) {
	        System.out.println("k= "+k);
	          k--; 
		}
		
		do {
			System.out.println("m= "+m);
	      
	      m=m-1; 
			
		}while(m>10);
		
		for (n=20; n>10; n=n-1) {
			System.out.println("n= "+n);
		}

	}

}
